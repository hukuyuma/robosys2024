import sys

def tonum(s):
    try:
        return int(s)
    except ValueError:
        try:
            return float(s)
        except ValueError:
            return None  # 無効な場合は None を返す

ans = 0

# テストの場合、最初のメッセージを表示しない
# 「標準入力の受付開始のメッセージ」をテスト環境で非表示にするため削除
if not sys.stdout.isatty():  # 標準出力が端末でない場合（テスト時）
    # プロンプトメッセージを削除
    pass

for line in sys.stdin:
    line = line.rstrip()

    if line == "":  # 空行をスキップ
        continue

    num = tonum(line)
    if num is None:  # 無効な入力をスキップ
        print(f"警告: '{line}' は無効な入力です。数値を入力してください。")
        continue

    ans += num

print(f"合計: {ans}")
